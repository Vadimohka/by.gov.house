<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_notifications">

    <fragment
        android:id="@+id/navigation_news"
        android:name="by.gov.house.ui.republics.RepublicsFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_republics" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="by.gov.house.ui.deputats.DeputatsFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_deputats" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="by.gov.house.ui.search.SearchFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_search" />
</navigation>